
#include <iostream>
#include<bits/stdc++.h>
using namespace std;

//Using MEMOIZATION

int fibonacci(int n,vector<int>&dp)   
{
    if(n==0)return 0;
    if(n==1)return 1;
    
    if(dp[n]!=-1)return dp[n];
    
    int left = fibonacci(n-1,dp);
    int right = fibonacci(n-2,dp);
    dp[n]=left+right;
    
    return dp[n];
    
}

int main()
{
    int n;
    cin>>n;
    vector<int>dp(n+1,-1);
    
    cout<<fibonacci(n,dp)<<endl;

    return 0;
}
