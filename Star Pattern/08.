n=3

1    1
12  12
123123

void solve(int n)
{ 
  for(int i=0;i<n;i++)
  {
      for(int j=0;j<=i;j++)
      {
          cout<<j+1<<"";
      }
      for(int j=(2*(n-1-i));j>0;j--)
      {
          cout<<" ";
      }
       for(int j=0;j<=i;j++)
      {
          cout<<j+1<<"";
      }
      cout<<endl;
  }
}
